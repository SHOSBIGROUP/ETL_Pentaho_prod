    <quoteString>N</quoteString>
    <sql>select div_no,itm_no,item_desc,sales,top20 from (												
select div_no,itm_no,item_desc,sales, 										
(rank()  over (partition by div_no order by div_no,sales desc)) as top20												
from (												
SELECT     --  top 20                 												
   a.DIV_OGP_NO as div_no,       a.PRD_ITM_NO as itm_no,    P.prd_ds as item_desc , 																								
 max(a.POS_SLS_TRS_AM/prd_qt)  as sales 												
FROM  												
   ( pos_dw_views.posxtpp_prd_lin a LEFT JOIN  lci_dw_views.lcixtfa_fty e ON a.fty_id_no = e.fty_id_no)																								
    join sprs_dw_views.product p on p.div_no=a.DIV_OGP_NO and a.PRD_ITM_NO=p.itm_no												
WHERE   a.fty_id_no = e.fty_id_no 												
        AND e.fty_sbt_cd IN ( 'L','L3','H','H3','O','O3',  'M')												
        and a.DIV_OGP_NO  in ('046','022','026','071')												
        AND A.TRS_DT >= date - 14												
        and a.POS_SLS_TRS_AM>0												
        group by   a.DIV_OGP_NO   ,   a.PRD_ITM_NO ,P.prd_ds) aa ) aaa												
        where top20&lt;=20												
        order by 1,4												
							</sql>
