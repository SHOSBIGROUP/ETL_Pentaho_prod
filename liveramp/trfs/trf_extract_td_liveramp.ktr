  <step>
    <name>teradata sales by customer1</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>liveramp_connect</connection>
    <sql>SELECT  DISTINCT  CASE WHEN  c.cus_lst_nm is not null  THEN c.cus_fst_nm ELSE 'a' END  AS First_Name, 
            CASE WHEN  c.cus_lst_nm is not null THEN c.cus_lst_nm ELSE 'a' END  AS Last_Name, 
                c.st_ln1_ad AS Address_1, 
                c.st_ln2_ad AS Address_2, 
                c.cty_nm    AS City, 
                c.ste_cd    AS State, 
                c.zip_cd    AS Zip, 
                  eml_ad    AS Email_1, 
               ' NULL'        AS Email_2, 
                'NULL'        AS Email_3, 
               ' NULL'        AS Email_4, 
                CASE 
                                WHEN format_sub_type LIKE 'L%' THEN 'instore-shos-outlet' 
                                WHEN format_sub_type LIKE 'O%' THEN 'instore-shos-hts' 
                                WHEN format_sub_type LIKE 'H%' THEN  'instore-shos-ahs'
                END       AS Product_Group, 
               CAST( TRS_DT AS VARCHAR(10)) || ' ' || CAST( TRS_TM AS VARCHAR(8)) AS Time_Stamp,
                trs_am AS Conversion_Amount 
              FROM            ( 
                       SELECT orig_facility_nbr AS fty_id_no, 
                              format_sub_type 
                       FROM   lci_dw_views.mstr_locn_tbl 
                       WHERE  format_sub_type LIKE   'L%'  OR 
                              format_sub_type LIKE   'O%' OR 
                              format_sub_type LIKE   'H%' 
   ) s 
INNER JOIN 
                ( 
                       SELECT fty_id_no, 
                              trs_dt, 
                              trs_tm, 
                              trs_am, 
                              tot_trs_dnt_am, 
                              cus_ian_id_no, 
                              unv_lyl_crd_no 
                       FROM   pos_dw_views. posxtpx_pos_tran 
                       WHERE  (trs_dt >=  date - 1  )
                       ) h 
ON              s.fty_id_no = h.fty_id_no 
LEFT OUTER JOIN 
 ( SELECT src_apl_cd, 
                              src_sys_idv_id_no, 
                              nn.crt_ts, 
                              nn.eml_ad_id ,
                              e.eml_ad 
                       FROM   lci_dw_views.lcixtex_src_eml  nn  , lci_dw_nadr_views.lcixtem_eml_ad  e 
WHERE  src_apl_cd = 'XUI'     AND    substring(src_sys_idv_id_no from 1 FOR 4)='7081'
       AND  nn.eml_ad_id = e.eml_ad_id ) n
ON              h.unv_lyl_crd_no = n.src_sys_idv_id_no
LEFT OUTER JOIN 
                (   SELECT cus_ian_id_no, 
                              cus_id_no, 
                              cus_lst_nm , 
                              cus_fst_nm , 
                              st_ln1_ad, 
                              st_ln2_ad, 
                              cty_nm, 
                              ste_cd, 
                              zip_cd, 
                              cus_phn_no, 
                              cus_phn_are_cd, 
                              cus_alt_phn_no, 
                              cus_alt_phn_are_cd 
                       FROM   pos_dw_nadr_views.posxtcn_cus_info ) c 
ON              h.cus_ian_id_no=c.cus_ian_id_no
;</sql>
